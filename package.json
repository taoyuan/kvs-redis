{
  "name": "kvs-redis",
  "version": "2.1.0",
  "description": "kvs-redis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/taoyuan/kvs-redis.git"
  },
  "author": "Yvan Tao <towyuan@outlook.com>",
  "keywords": [
    "kv",
    "kvs",
    "key",
    "value",
    "key-value",
    "cache",
    "memory",
    "store",
    "redis"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prerelease": "yarn install && cross-env CI=1 yarn test",
    "release": "yarn prerelease && np --no-2fa --no-cleanup --no-tests --no-yarn",
    "build": "nb-tsc -b",
    "build:full": "yarn install && yarn clean && yarn build",
    "lint": "yarn prettier:check && yarn eslint",
    "lint:fix": "yarn eslint:fix && yarn prettier:fix",
    "eslint": "nb-eslint --report-unused-disable-directives --cache .",
    "eslint:fix": "yarn eslint --fix",
    "prettier:cli": "nb-prettier \"**/*.ts\" \"**/*.js\" \"**/*.md\"",
    "prettier:check": "yarn prettier:cli --check",
    "prettier:fix": "yarn prettier:cli --write",
    "clean": "nb-clean 'dist' 'coverage' '.nyc_output' '*.tsbuildinfo'",
    "pretest": "yarn clean && yarn build",
    "test": "yarn jest",
    "test:ci": "jest --collectCoverage --coverageDirectory=\"./coverage\" --reporters=default --reporters=jest-junit --watchAll=false",
    "jest": "jest",
    "posttest": "yarn lint"
  },
  "dependencies": {
    "kvs": "^2.3.0",
    "redis": "^4.2.0",
    "tily": "^2.16.1"
  },
  "devDependencies": {
    "@nutol/build": "^0.2.10",
    "@nutol/eslint-config": "^0.2.5",
    "@types/jest": "^28.1.6",
    "@types/micromatch": "^4.0.2",
    "@types/node": "^18.0.6",
    "@types/redis-mock": "^0.17.1",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-plugin": "^5.0.1",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "jest-junit": "^14.0.0",
    "kvs-testlab": "^0.3.0",
    "np": "^7.6.2",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "redis-mock": "^0.56.3",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "packageManager": "yarn@3.2.2"
}
